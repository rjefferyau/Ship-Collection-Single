{
  "name": "collecthub-multi-tenant",
  "version": "1.0.0-beta.1",
  "description": "Multi-tenant Starship Collection Manager with Supabase authentication and PostgreSQL database",
  "private": true,
  "keywords": ["starship", "collection", "manager", "multi-tenant", "supabase", "postgresql", "nextjs"],
  "repository": {
    "type": "git",
    "url": "https://github.com/rjefferyau/Ship-Collection-Multi-Tenant.git"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:fast": "cross-env NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    
    "docker:setup": "./scripts/setup-local-env.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "./scripts/reset-database.sh",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:app": "docker-compose logs -f app",
    "docker:logs:db": "docker-compose logs -f db",
    
    "supabase:start": "docker-compose up -d db auth rest studio",
    "supabase:stop": "docker-compose stop",
    "supabase:status": "docker-compose ps",
    "supabase:studio": "open http://localhost:3002",
    
    "db:migrate": "docker-compose exec db psql -U postgres -d postgres -f /docker-entrypoint-initdb.d/migrations/",
    "db:seed": "docker-compose exec db psql -U postgres -d postgres -f /docker-entrypoint-initdb.d/seed.sql",
    "db:reset": "npm run docker:reset",
    
    "migrate:export": "node scripts/export-mongodb-data.js",
    "migrate:transform": "node scripts/transform-data.js", 
    "migrate:import": "node scripts/import-to-postgresql.js",
    "migrate:validate": "node scripts/validate-migration.js",
    
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:rls": "npm run test -- --testMatch='**/rls/**/*.test.js'",
    
    "types:generate": "supabase gen types typescript --local > types/database.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "next": "15.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-hook-form": "^7.43.9",
    "postgres": "^3.4.3",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.13.2",
    "framer-motion": "^10.16.4",
    "recharts": "^2.7.2",
    "react-dropzone": "^14.2.3",
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
    "react-hot-toast": "^2.4.1",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@supabase/cli": "^1.127.4",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/uuid": "^9.0.7",
    "@types/papaparse": "^5.3.7",
    "@types/pg": "^8.10.9",
    "typescript": "^5.1.6",
    "tailwindcss": "^3.3.3",
    "postcss": "^8.4.27",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "prettier": "^3.0.0",
    "jest": "^29.6.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "jest-environment-jsdom": "^29.6.2",
    "@playwright/test": "^1.37.1",
    "rimraf": "^5.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}